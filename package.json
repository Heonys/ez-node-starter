{
  "name": "ts-eznode-starter",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build-css": "shx mkdir -p dist/src/public && shx cp -r src/public/main.css dist/src/public/main.css",
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run build-css",
    "serve": "cross-env NODE_ENV=production PORT=8080 node dist/src/server.js",
    "pm2": "cross-env NODE_ENV=production PORT=8080 pm2 start dist/src/server.js -i 0",
    "start": "npm run serve",
    "dev": "concurrently \"tsc -w\" \"cross-env NODE_ENV=development PORT=8080 nodemon dist/src/server.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
